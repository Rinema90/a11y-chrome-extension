{
    "name": "Extension Accessibilit√©",
    "description": "Extension pour rendre les sites plus accessibles",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons":{
        "16": "icone16.png",
        "32": "icone32.png",
        "128": "icone128.png"
    },

    "content_scripts": [
    {
        "run at":"document_end",
        "matches": [
            "https://*.wikipedia.org/*"
        ],
        "js": ["jquery.min.js","contentFinal.js"]
    }
    ],
    "action":{
        "default_popup":"popupFinal.html"
    }
}