{
    "name": "a11y Chrome extension",
    "description": "Chrome extension to make Wikip√©dia more accessible",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons":{
        "16": "icons/icone16.png",
        "32": "icons/icone32.png",
        "128": "icons/icone128.png"
    },

    "content_scripts": [
    {
        "run at":"document_end",
        "matches": [
            "https://*.wikipedia.org/*"
        ],
        "js": ["js/jquery.min.js","js/applyStyles.js"]
    }
    ],
    "action":{
        "default_popup":"popup.html"
    }
}